{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": ["src/**/*.astro", "src/**/*.ts", "src/**/*.tsx", ".astro/types.d.ts"],
  "exclude": ["dist", "node_modules"]
}
